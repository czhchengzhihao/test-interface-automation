<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.czh.automation.mapper.responseresult.ResponseResultMapper">
    <select id="queryResponseResult" resultType="com.czh.automation.entity.ResponseResult">
        SELECT success,message,responseData
        from response_result where remark=#{remark}
    </select>
</mapper>